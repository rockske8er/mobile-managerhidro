{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@Assets/*"    : ["./src/assets/*"],
      "@Components/*": ["./src/components/*"],
      "@Routes/*"    : ["./src/routes/*"],
      "@Screens/*"   : ["./src/screens/*"],
      "@Services/*"  : ["./src/services/*"] ,
      "@Themes/*"    : ["./src/themes/*"],
    }
  },
}
